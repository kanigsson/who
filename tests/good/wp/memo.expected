logic /\  : prop -> prop -> prop
logic \/  : prop -> prop -> prop
logic ->  : prop -> prop -> prop
logic = ['a||] : a -> a -> prop
logic mk_2tuple ['a 'b||] : a -> b -> a * b
type bool
