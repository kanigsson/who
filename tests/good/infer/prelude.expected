                  {(fun (cur6:< 'e6 >) ->
                   (inv start cur6) /\
                   (forall (i:int).
                    ((start <= i) /\ (i <= end_)) ->
                    (forall (m:< 'e6 >).
                     (inv i m) ->
                     ((pre [int -> <{|e6}> -> prop,int -> <{|e6}> ->
                       <{|e6}> -> (unit) -> prop||] f i m)
                      /\
                      (forall (n:< 'e6 >).
                       (post [int -> <{|e6}> -> prop,int -> <{|e6}> ->
                        <{|e6}> -> (unit) -> prop||] f i m n tt)
                       -> (inv (i + 1) n))))))}
                  parameter(unit,{ 'e6 })
                  {(fun (old3:< 'e6 >) ->
                   (fun (cur7:< 'e6 >) ->
                   (fun (anon1:unit) -> inv (int_max start (end_ + 1)) cur7)))})))) 
