let
  iter ['a|t|'e] = (fun (inv:<t 'e> -> int -> prop) ->
                   (fun (ar:ref(t,array[a])) ->
                   (fun (f:a ->{  + t 'e} unit) ->
                   {(fun (cur:<t 'e>) ->
                    (inv cur 0) /\
                    (forall (i:int).
                     ((0 <= i) /\ (i < (len [a||] !!ar@cur))) ->
                     ([[(fun (cur1:<t 'e>) -> inv cur1 i)]]f
                                                           (get [a||] i
                                                            (! [array[a]| t|]
                                                             ar))[[(fun (old:<t 'e>)
                                                                   ->
                                                                   (fun (cur1:<t 'e>)
                                                                   ->
                                                                   (fun (anon:unit)
                                                                   ->
                                                                   inv cur1
                                                                   (i + 1))))]])))}
                   let __start = 0 in
                   let __end = (len [a||] (! [array[a]| t|] ar)) - 1 in
                   forto [|| { } {t 'e}]
                   (fun (i:int) -> (fun (cur:<t 'e>) -> inv cur i)) __start
                   __end
                   (fun (i:int) ->
                   {(fun (cur:<t 'e>) ->
                    ((__start <= i) /\ (i <= __end)) /\ (inv cur i))}
                   f (get [a||] i (! [array[a]| t|] ar))
                   {(fun (anon:<t 'e>) ->
                    (fun (cur:<t 'e>) ->
                    (fun (anon1:unit) -> inv cur (i + 1))))})
                   {(fun (old:<t 'e>) ->
                    (fun (cur:<t 'e>) ->
                    (fun (anon:unit) -> inv cur (len [a||] !!ar@old))))}))) 
