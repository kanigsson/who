parameter forto [||'e] (inv : int -> <'e> -> prop) (start end_ : int) 
  (f : int ->{'e} unit) : unit, {'e} =
    { inv start cur /\
          forall (i : int). start <= i /\ i <= end_ ->
          forall (m : <'e>) . inv i m -> pre f i m /\
          forall (n : <'e>). post f i m n () -> inv (i+1) n
    }
    { inv (int_max start (end_ + 1)) cur} 
