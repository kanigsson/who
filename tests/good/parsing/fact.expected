axiom fact_zero : (fact 0) = 1
axiom fact_step : forall (n:int).
                  (0 <= n) -> ((fact (n + 1)) = (n * (fact n)))
let
  rec(int ->{ } int) f  = (fun (x:int) -> {0 <= x}
