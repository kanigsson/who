logic !! ['a|r|'e] : ref(r,a) -> <r 'e> -> a
let
  ! ['a|r|] = (fun (x:ref(r,a)) -> {(fun (cur:<r >) -> True)}
              parameter(a,{r })
              {(fun (old:<r >) ->
               (fun (cur:<r >) ->
               (fun (r1:a) ->
               ((!! [a| r| { }] x cur) =[a||] r1) /\
               ((!! [a| r| { }] x old) =[a||] (!! [a| r| { }] x cur)))))}) 
let
  := ['a|r|] = (fun (x:ref(r,a)) ->
               (fun (v:a) -> {(fun (cur:<r >) -> True)} parameter(unit1,{r })
               {(fun (old:<r >) ->
                (fun (cur:<r >) ->
                (fun (anon:unit1) -> (!! [a| r| { }] x cur) =[a||] v)))})) 
let
  ref ['a|r|] = (fun (v:a) ->allocates r {(fun (cur:<r >) -> True)}
                parameter(ref(r,a),{r })
                {(fun (old:<r >) ->
                 (fun (cur:<r >) ->
                 (fun (x:ref(r,a)) -> (!! [a| r| { }] x cur) =[a||] v)))}) 
