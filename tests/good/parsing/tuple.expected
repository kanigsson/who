axiom int_max_is_ge:
  forall (x:int).
  forall (y:int). ((int_max x y) >= x) /\ ((int_max x y) >= y) 
axiom int_max_is_some:
  forall (x:int).
  forall (y:int). ((int_max x y) =[int||] x) \/ ((int_max x y) =[int||] y) 
axiom int_min_is_le:
  forall (x:int).
  forall (y:int). ((int_min x y) <= x) /\ ((int_min x y) <= y) 
axiom int_min_is_some:
  forall (x:int).
  forall (y:int). ((int_min x y) =[int||] x) \/ ((int_min x y) =[int||] y) 
