let
  forto [||'e] = (fun (inv:int -> < 'e> -> prop) ->
                 (fun (start:int) ->
                 (fun (end_:int) ->
                 (fun (f:int ->{ 'e} unit1) ->
                 {(fun (cur:< 'e>) ->
                  (inv start cur) /\
                  (forall (i:int).
                   ((start <= i) /\ (i <= end_)) ->
                   (forall (m:< 'e>).
                    (inv i m) ->
                    ((pre [int -> < 'e> -> prop,int -> < 'e> -> < 'e> ->
                      unit1 -> prop||] f i m)
                     /\
                     (forall (n:< 'e>).
                      (post [int -> < 'e> -> prop,int -> < 'e> -> < 'e> ->
                       unit1 -> prop||] f i m n tt)
                      -> (inv (i + 1) n))))))}
                 parameter(unit1,{ 'e})
                 {(fun (old:< 'e>) ->
                  (fun (cur:< 'e>) ->
                  (fun (anon:unit1) -> inv (int_max start (end_ + 1)) cur)))})))) 
let
  fordownto [||'e] = (fun (inv:int -> < 'e> -> prop) ->
                     (fun (start:int) ->
                     (fun (end_:int) ->
                     (fun (f:int ->{ 'e} unit1) ->
                     {(fun (cur:< 'e>) ->
                      (inv start cur) /\
                      (forall (i:int).
                       ((end_ <= i) /\ (i <= start)) ->
                       (forall (m:< 'e>).
                        (inv i m) ->
                        ((pre [int -> < 'e> -> prop,int -> < 'e> -> < 'e> ->
                          unit1 -> prop||] f i m)
                         /\
                         (forall (n:< 'e>).
                          (post [int -> < 'e> -> prop,int -> < 'e> ->
                           < 'e> -> unit1 -> prop||] f i m n tt)
                          -> (inv (i - 1) n))))))}
                     parameter(unit1,{ 'e})
                     {(fun (old:< 'e>) ->
                      (fun (cur:< 'e>) ->
                      (fun (anon:unit1) ->
                      inv (int_min start (end_ - 1)) cur)))})))) 
