let
  assert  = (fun (f:prop) -> {(fun (cur:< >) -> f)} parameter(unit1,{ })
            {(fun (old:< >) -> (fun (cur:< >) -> (fun (anon:unit1) -> f)))}) 
let
  check  = (fun (f:prop) -> {(fun (cur:< >) -> f)} parameter(unit1,{ })
           {(fun (old:< >) -> (fun (cur:< >) -> (fun (anon:unit1) -> True)))}) 
