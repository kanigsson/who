axiom update_length:
  forall ['a||]. forall (t:array[a]).
                 forall (i:int).
                 forall (z:a).
                 ((0 <= i) /\ (i < (len [a||] t))) ->
                 ((len [a||] t) =[int||] (len [a||] (set [a||] i z t))) 
axiom get_set_eq:
  forall ['a||]. forall (t:array[a]).
                 forall (i:int).
                 forall (z:a).
                 ((0 <= i) /\ (i < (len [a||] t))) ->
                 ((get [a||] i (set [a||] i z t)) =[a||] z) 
axiom length_empty: forall ['a||]. (len [a||] ar_empty [a||]) =[int||] 0 
axiom get_set_neq:
  forall ['a||]. forall (t:array[a]).
                 forall (i:int).
                 forall (j:int).
                 forall (z:a).
                 ((0 <= i) /\ (i < (len [a||] t))) ->
                 (((0 <= j) /\ (j < (len [a||] t))) ->
                  ((i <>[int||] j) ->
                   ((get [a||] i (set [a||] j z t)) =[a||] (get [a||] i t)))) 
axiom length_nonnegative:
  forall ['a||]. forall (t:array[a]). 0 <= (len [a||] t) 
axiom length_create:
  forall ['a||]. forall (l:int).
                 forall (v:a).
                 (0 <= l) -> ((len [a||] (create [a||] l v)) =[int||] l) 
axiom create_access:
  forall ['a||]. forall (i:int).
                 forall (l:int).
                 forall (v:a).
                 (0 <= i) ->
                 ((i < l) -> ((get [a||] i (create [a||] l v)) =[a||] v)) 
