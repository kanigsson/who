logic = ['a||] : a -> a -> prop
logic /\  : prop -> prop -> prop
logic !! ['a|r|'e] : ref(r,a) -> <r 'e> -> a
let
  ! ['a|r|] = (fun (x:ref(r,a)) -> {(fun (cur:<r >) -> True)}
              parameter(a,{  + r })
              {(fun (old:<r >) ->
               (fun (cur:<r >) ->
               (fun (r1:a) -> (!!x@cur = r1) /\ (!!x@old = !!x@cur))))}) 
let seq ['a 'b 'c||'e
  'e1] = (fun (f:a ->{  +  'e} b) ->
         (fun (g:b ->{  +  'e1} c) ->
         (fun (x:a) -> {(fun (cur:< 'e 'e1>) -> True)} g (f x)
         {(fun (old:< 'e 'e1>) ->
          (fun (cur:< 'e 'e1>) -> (fun (anon:c) -> True)))}))) 
