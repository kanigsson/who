logic = ['a||] : a -> a -> prop
logic /\  : prop -> prop -> prop
logic ->  : prop -> prop -> prop
logic fst ['a1 'b||] : (a1 * b) -> a1
logic snd ['a2 'b1||] : (a2 * b1) -> b1
logic !! ['a3|r|'e] : ref(r,a3) -> <{r  'e }> -> a3
let
  ! ['a4|r1|] = (fun (x:ref(r1,a4)) -> {(fun (cur:<{r1  }>) -> True)}
                parameter(a4,{r1  })
                {(fun (old:<{r1  }>) ->
                 (fun (cur1:<{r1  }>) ->
                 (fun (r2:a4) ->
                 ((x !![a4| r1| { }] cur1) =[a4||] r2) /\
                 ((x !![a4| r1| { }] old) =[a4||] (x !![a4| r1| { }] cur1)))))}) 
